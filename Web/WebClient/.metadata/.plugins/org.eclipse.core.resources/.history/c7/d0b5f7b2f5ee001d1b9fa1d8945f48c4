	<script type="text/javascript">
		/* 
			JAVA
			public void print(){}
			public String print(){return "";}
			public int print (int num, int num2) {return num + num2;}
			
			Javascript
			function 함수 () {}
			function 함수 (num, num2) {}
			function 함수 () {return 100;}
			
			void(x), return type(x)
			
		*/
		
		function callConfirm(){  //사용자 정의 함수
			if(window.confirm("삭제할까요..")){ //confirm 내장함수 return true , false
				alert("네");
			}else{
				alert("아니오");
			}
		}
	</script>
</head>
<body>
	<h3>내장 함수 , 사용자 정의 함수</h3>
