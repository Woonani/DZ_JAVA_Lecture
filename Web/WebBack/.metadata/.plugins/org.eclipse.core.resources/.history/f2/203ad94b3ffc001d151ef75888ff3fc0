package test;

import java.util.HashMap;

public class ForProgrammers {

	
	public static void main(String[] args) {
		
		HashMap<String, String> morse2 = new HashMap<>();
		
		morse2.put(".-","a");
		
		// =====================================
		
		String morse = "'.-':'a','-...':'b','-.-.':'c','-..':'d','.':'e','..-.':'f',"
				+ "    '--.':'g','....':'h','..':'i','.---':'j','-.-':'k','.-..':'l',"
				+ "    '--':'m','-.':'n','---':'o','.--.':'p','--.-':'q','.-.':'r',"
				+ "    '...':'s','-':'t','..-':'u','...-':'v','.--':'w','-..-':'x',"
				+ "    '-.--':'y','--..':'z'";
		
		String[] mArr1 = morse.split(",");
		
		for(String a : mArr1) {
			
			System.out.println(a);
		}

	}

}
